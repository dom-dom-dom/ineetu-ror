<h2>View All Lists</h2>

<% @user.lists.each do |list| %>
  <p>
    <strong>List Title:</strong>
    <%= list.title %>
  </p>
 
  <p>
    <strong>List Description:</strong>
    <%= list.description %>
  </p>

  <%= link_to 'Edit', edit_list_path(list) %> |
  <%= link_to 'Destroy', list_path(list), method: :delete, data: { confirm: 'Are you sure?' } %>
  
<% end %>