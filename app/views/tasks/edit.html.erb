<h2>Edit Tasks</h2>

<%= form_for [@list, @task] do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :priority %><br>
    <%= f.text_area :priority %>
  </p>

 
  <p>
    <%= f.submit %>
  </p>
<% end %>